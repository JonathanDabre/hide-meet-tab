{
  "manifest_version": 3,
  "name": "Hide Meet Tab",
  "version": "1.0",
  "description": "Move Google Meet tab to a hidden window and bring it back.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon48.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js" 
  }
}
